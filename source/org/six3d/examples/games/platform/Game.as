package org.six3d.examples.games.platform {	import flash.display.MovieClip;	import flash.display.Sprite;		import org.six3d.display.DisplayObject3D;	import org.six3d.display.Scene3D;	import org.six3d.primitives.Circle3D;	import org.six3d.primitives.Rectangle3D;	import org.six3d.utils.FPSUtil;		import caurina.transitions.Tweener;	/**	 * @private	 * @author Robert Sk√∂ld, bob@six3d.org	 */	public class Game extends Sprite {				public var dude : MovieClip;		private var _scene : Scene3D;		private var _root : DisplayObject3D;		public function Game() {			stage.align = "TL";			stage.scaleMode = "noScale";						setupScene();			setupObjects();						new FPSUtil( this);		}		private function setupScene() : void {			var container3d : Sprite = new Sprite();			container3d.x = 250;			container3d.y = 200;			addChild( container3d );			_scene = new Scene3D( container3d );			_root = _scene.addChild( new DisplayObject3D( "root" ) );		}				private function setupObjects() : void {			var man : DisplayObject3D = new DisplayObject3D();			_scene.addChild( man );						var head : Circle3D = new Circle3D( 30 );			man.addChild( head );						var body : Rectangle3D = new Rectangle3D( 2 , 50 );			body.y = 30;			man.addChild( body );						var legLeft : Rectangle3D = new Rectangle3D( 2 , 50 );			legLeft.y = 80;			legLeft.rotationZ = 30;			man.addChild( legLeft );						var legRight : Rectangle3D = new Rectangle3D( 2 , 50 );			legRight.y = 80;			legRight.rotationZ = -30;			man.addChild( legRight );						var armLeft : Rectangle3D = new Rectangle3D( 2 , 30 );			armLeft.y = 50;			armLeft.rotationZ = 60;			man.addChild( armLeft );						var armRight : Rectangle3D = new Rectangle3D( 2 , 30 );			armRight.y = 50;			armRight.rotationZ = -60;			man.addChild( armRight );						Tweener.addTween( head , { rotationX: 30 , y: 20 , time: 4 } );			Tweener.addTween( armRight , { rotationY: 70 , time: 4 } );			Tweener.addTween( legRight , { rotationY: 70 , rotationX: 30 , time: 4 } );		}			}}