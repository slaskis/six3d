package org.six3d.geom {	import flash.geom.Point;			/**	 * @author Robert Sk√∂ld, bob@six3d.org	 */	public class QuadBezier {		private var _p2 : Point;		private var _p1 : Point;		private var _c : Point;		public function QuadBezier( p1 : Point , c : Point , p2 : Point ) {			_p1 = p1;			_p2 = p2;			_c = c;		}				public function get start() : Point {			return _p1;		}		public function get control() : Point {			return _c;		}		public function get end() : Point {			return _p2;		}				public function toPathArray() : Array {			return [ control.x , control.y , end.x , end.y ];		}				public function toString() : String {			return "[QuadBezier start: " + start + ", control: " + control + ", end: " + end + "]";		}			}}