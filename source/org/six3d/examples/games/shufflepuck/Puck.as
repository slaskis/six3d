package org.six3d.examples.games.shufflepuck {	import org.cove.ape.AbstractParticle;	import org.six3d.display.Sprite3D;	import org.six3d.primitives.Circle3D;			/**	 * @private	 * @author Robert Sk√∂ld, bob@six3d.org	 */	public class Puck extends AbstractParticle {				private var _root : Sprite3D;				public function Puck( root : Sprite3D ) {			super( 10 , 10 , false , 1 , 1 , 0 );			_root = root;			setupPuck();		}				private function setupPuck() : void {			var circle : Circle3D = new Circle3D( 30 );			circle.alpha = .5;			_root.addChild( circle );						setDisplay( circle.sprite );		}	}}