package org.six3d.display {	import org.six3d.display.DisplayObject3D;		/**	 * @author Robert Sk√∂ld, bob@six3d.org	 */	public class Shape3D extends DisplayObject3D {		public function Shape3D( name : String = null ) {			super( name );		}		public function addMotif(motif : Array) : void {			_motif = _motif.concat(motif);			askRendering( true );		}				public override function toString() : String {			return "[Shape3D " + name + "]";		}	}}